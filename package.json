{
  "name": "react-router-redux-example",
  "description": "react-router-redux example with universal rendering",
  "version": "1.0.1",
  "license": "MIT",
  "author": "Steven Iseki <stevenisekimartin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenIseki/react-router-redux-example.git"
  },
  "scripts": {
    "build": "babel-node ./node_modules/.bin/webpack",
    "prestart": "rimraf public",
    "server": "babel-node ./src/server/main.js",
    "start": "concurrently 'npm run watch' 'npm run watch:server'",
    "watch": "npm run build -- --watch",
    "watch:server": "nodemon -x npm run server -w server.js -w src"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0",
    "koa-cash": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-send": "^3.2.0",
    "koa-static": "^3.0.0",
    "koa-useragent": "^0.1.4",
    "normalize-css": "^2.3.1",
    "object-hash": "^1.1.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redux": "^3.5.1",
    "redux-form": "^5.2.3",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "reflexbox": "^1.1.2",
    "serialize-javascript": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "compression-webpack-plugin": "^0.3.1",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^2.1.3",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.0"
  }
}
